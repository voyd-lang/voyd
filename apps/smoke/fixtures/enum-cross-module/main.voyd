use src::drinks::{ Drink, make_coffee }
use Drink::all

pub fn main() -> i32
  let brewed: Drink<i32> = make_coffee(12)
  let poured: Drink<i32> = Drink::Coffee<i32> { size: 8 }
  match(brewed)
    Coffee { size }:
      match(poured)
        Coffee { size: poured_size }:
          size + poured_size
        Tea:
          0
        Water:
          0
    Tea:
      0
    Water:
      0
