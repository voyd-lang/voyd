{
  "name": "@voyd/lib",
  "version": "0.9.0",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "development": "./src/lib/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./*.js": {
      "types": "./dist/*.d.ts",
      "development": "./src/lib/*.ts",
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "development": "./src/lib/*.ts",
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "vitest run --config ../../vitest.config.ts src/lib/__tests__",
    "lint": "echo 'no lint configured'"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.1.2",
    "@voyd/std": "*",
    "binaryen": "^125.0.0",
    "commander": "^14.0.0"
  }
}
