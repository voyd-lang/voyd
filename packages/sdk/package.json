{
  "name": "@voyd/sdk",
  "version": "0.9.0",
  "description": "Voyd SDK",
  "type": "module",
  "main": "./dist/node.js",
  "exports": {
    ".": {
      "types": "./dist/node.d.ts",
      "development": "./src/node.ts",
      "import": "./dist/node.js",
      "default": "./dist/node.js"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "development": "./src/browser.ts",
      "import": "./dist/browser/index.js",
      "default": "./dist/browser/index.js"
    },
    "./deno": {
      "types": "./dist/deno.d.ts",
      "development": "./src/deno.ts",
      "import": "./dist/deno.js",
      "default": "./dist/deno.js"
    },
    "./compiler": {
      "types": "./dist/compiler.d.ts",
      "development": "./src/compiler.ts",
      "import": "./dist/compiler.js",
      "default": "./dist/compiler.js"
    },
    "./js-host": {
      "types": "./dist/js-host.d.ts",
      "development": "./src/js-host.ts",
      "import": "./dist/js-host.js",
      "default": "./dist/js-host.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json && vite build -c ../../vite.config.ts",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "vitest run --config ../../vitest.config.ts --testTimeout 30000 src",
    "test:w": "vitest --config ../../vitest.config.ts src",
    "lint": "echo 'no lint configured'"
  },
  "dependencies": {
    "@voyd/compiler": "*",
    "@voyd/js-host": "*",
    "@voyd/std": "*",
    "@voyd/lib": "*",
    "binaryen": "^125.0.0"
  }
}
