use std::array::Array

pub fn entry_tail() -> i32
  let arr = Array<i32>::with_capacity(2)
  let arr = arr.push(1)
  let arr = arr.push(2)
  let storage = arr.raw_storage()
  __array_get(storage, 1)

pub fn fixed_roundtrip() -> i32
  let arr = Array<i32>::with_capacity(3)
  let arr = arr.push(1)
  let arr = arr.push(2)
  let arr = arr.push(3)
  let fixed = arr.to_fixed_array()
  __array_len(fixed)
