eff Async
  await(resume, value: i32) -> i32

fn inner(x: i32): Async -> i32
  let y = Async::await(x)
  y + 1

pub fn outer(): Async -> i32
  let a = inner(5)
  a + 2

pub fn outer_twice(): Async -> i32
  let a = inner(1)
  let b = inner(2)
  a + b

pub fn outer_nested(): Async -> i32
  let x = outer()
  x + 10
