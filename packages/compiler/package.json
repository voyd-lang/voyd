{
  "name": "@voyd/compiler",
  "version": "0.9.0",
  "description": "Voyd compiler",
  "type": "module",
  "main": "./dist/pipeline.js",
  "exports": {
    ".": {
      "types": "./dist/pipeline.d.ts",
      "development": "./src/pipeline.ts",
      "import": "./dist/pipeline.js",
      "default": "./dist/pipeline.js"
    },
    "./*.js": {
      "types": "./dist/*.d.ts",
      "development": "./src/*.ts",
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "vitest run --config ../../vitest.config.ts --testTimeout 10000 src",
    "bench:typing": "vitest bench --config ../../vitest.config.ts src/semantics/typing/__tests__/typing.bench.ts",
    "test:w": "vitest --config ../../vitest.config.ts --testTimeout 10000 src",
    "lint": "echo 'no lint configured'"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.1.2",
    "@voyd/lib": "*"
  },
  "devDependencies": {
    "@voyd/js-host": "*"
  }
}
