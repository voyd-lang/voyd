@effect(id: "std::std::test::assertions::Test")
pub eff Test
  fail(resume) -> void
  skip(resume) -> void
  log(resume) -> void

/// Fails the current test when the assertion condition is not met.
pub fn assert(cond: boolean): fail -> void
  if not cond:
    Test::fail()

/// Fails the current test when the assertion condition is not met.
pub fn assert<T>(value: T, { eq expected: T }): fail -> void
  if value != expected:
    Test::fail()

/// Fails the current test when the assertion condition is not met.
pub fn assert<T>(value: T, { neq expected: T }): fail -> void
  if value == expected:
    Test::fail()
