use std::array::Array
use std::result::types::{ Result }
use self::type::self as string_type
pub use self::type::{
  String,
  StringSlice,
  StringIndex,
  Utf8Error,
  ParseIntError,
  ParseFloatError
}

pub fn new_string(from_bytes: FixedArray<i32>): () -> String
  string_type::new_string(from_bytes)

pub fn from_utf8(bytes: Array<i32>): () -> Result<String, Utf8Error>
  string_type::from_utf8(bytes)
