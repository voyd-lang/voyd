//! Math APIs for integer, float, interpolation, constants, and math errors.
//! Import through `std::math` for namespaced calls (for example,
//! `math::pow(2.0, 3.0)`), or import `math::all` to enable UFCS-style calls
//! such as `2.0.pow(3.0)`.
pub self::constants
pub self::errors
pub self::float
pub self::int
pub self::interpolate

pub use self::errors::{ MathError }
pub use self::constants::all
pub use self::float::all
pub use self::int::all
pub use self::interpolate::all
