fn applyTwice(f: (i32) -> i32, value: i32) -> i32
  f(f(value))

fn makeIncrementer(delta: i32) -> ((i32) -> i32)
  (value) => value + delta

pub fn main() -> i32
  let doubled = (x) => x * 2
  let result = applyTwice(doubled, 3)
  let inc = makeIncrementer(1)
  let trimmed = ((n) => n - 1)(result)
  trimmed + inc(4)
