obj Some<T> {
  value: T
}

obj None {}

obj Box { val: i32 }

type Optional<T> = Some<T> | None

type Branch = Optional<Node> | Box

obj Node {
  value: i32,
  left: Branch,
  right: Branch
}

fn extract(branch: Branch, sum: i32) -> i32
  branch.match
    Some<Node>: work(branch.value, sum)
    None:
      0
    Box:
      branch.val

fn work(node: Node, sum: i32) -> i32
  let left = extract(node.left, sum)
  let right = extract(node.right, sum)
  node.value + sum + left + right

pub fn main() -> i32
  let node = Node {
    value: 3,
    left: Box { val: 5 },
    right: Some {
      value: Node {
        value: 2,
        left: None {},
        right: None {}
      }
    }
  }
  work(node, 0)
