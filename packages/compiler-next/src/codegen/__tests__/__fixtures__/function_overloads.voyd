fn add(a: i32, b: i32)
  a + b

fn add(a: f64, b: f64)
  a + b

pub fn call_int() -> i32
  add(1, 2)

pub fn call_float() -> f64
  add(1.0, 2.0)

fn route(a: i32, { from dest: i32 }) -> i32
  a - dest

fn route(a: i32, { to dest: i32 }) -> i32
  a + dest

pub fn call_from() -> i32
  route(1, from: 2)

pub fn call_to() -> i32
  route(1, to: 2)
