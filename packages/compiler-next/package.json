{
  "name": "@voyd/compiler-next",
  "version": "0.9.0",
  "description": "Voyd next-gen compiler experiments",
  "type": "module",
  "main": "./dist/compiler-next/src/pipeline.js",
  "exports": {
    ".": {
      "types": "./src/pipeline.ts",
      "development": "./src/pipeline.ts",
      "import": "./dist/compiler-next/src/pipeline.js",
      "default": "./dist/compiler-next/src/pipeline.js"
    },
    "./*": {
      "types": "./src/*",
      "development": "./src/*",
      "import": "./dist/compiler-next/src/*",
      "default": "./dist/compiler-next/src/*"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "vitest run --config ../../vitest.config.ts src",
    "test:w": "vitest --config ../../vitest.config.ts src",
    "lint": "echo 'no lint configured'"
  },
  "dependencies": {
    "@voyd/lib": "0.9.0"
  }
}
