pub obj Version {
  api major: i32,
  api minor: i32,
  api patch: i32
}

impl Version
  fn init(major: i32, minor: i32, patch: i32)
    Version { major, minor, patch }

  api fn bump_major(version: Version) -> Version
    Version(version.major + 1, 0, 0)

  api fn bump_minor(version: Version) -> Version
    Version(version.major, version.minor + 1, 0)

  api fn bump_patch(version: Version) -> Version
    Version(version.major, version.minor, version.patch + 1)
