use std::all
use std::msg_pack::MsgPack

pub fn create_element(
  name: String,
  attributes: Array<(String, MsgPack)>,
  children: Array<MsgPack>
) -> Map<MsgPack>
  let attributeMap = Map<MsgPack>(attributes)
  Map<MsgPack>([
    ("name", name),
    ("attributes", attributeMap),
    ("children", children)
  ])
